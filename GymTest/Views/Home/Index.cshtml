@{
    ViewData["Title"] = "Administrador de Gym";
}
<br />

<script>function fingerPrimtValue(val) {
        document.getElementById("fingerprint").value += val;
    }

    function deleteLasFingerValue() {
        document.getElementById("fingerprint").value = document.getElementById("fingerprint").value.substring(0, document.getElementById("fingerprint").value.length - 1);
    }</script>


<div>
    <form asp-controller="Home" asp-action="Contact" asp-antiforgery="false">
        <p>
            <input type="text" name="Fingerprint" id="fingerprint" autofocus style="position: relative;width: 60%; height:80px; font-size:200%">
            <input type="submit" value="Entrar" class="btn btn-success btn-lg" style="position: relative;width: 30%; height:100px; font-size:200%" />
        </p>
    </form>
</div>

<div>
    <p>
        <input type="button" onclick="fingerPrimtValue(1)" value="1" class="btn btn-info btn-lg" style="position:relative; width:30%; height:100px; font-size:500%" />
        <input type="button" onclick="fingerPrimtValue(2)" value="2" class="btn btn-info btn-lg" style="position:relative; width:30%; height:100px; font-size:500%" />
        <input type="button" onclick="fingerPrimtValue(3)" value="3" class="btn btn-info btn-lg" style="position:relative; width:30%; height:100px; font-size:500%" />
    </p>
    <p>
        <input type="button" onclick="fingerPrimtValue(4)" value="4" class="btn btn-info btn-lg" style="position:relative; width:30%; height:100px; font-size:500%" />
        <input type="button" onclick="fingerPrimtValue(5)" value="5" class="btn btn-info btn-lg" style="position:relative; width:30%; height:100px; font-size:500%" />
        <input type="button" onclick="fingerPrimtValue(6)" value="6" class="btn btn-info btn-lg" style="position:relative; width:30%; height:100px; font-size:500%" />
    </p>
    <p>
        <input type="button" onclick="fingerPrimtValue(7)" value="7" class="btn btn-info btn-lg" style="position:relative; width:30%; height:100px; font-size:500%" />
        <input type="button" onclick="fingerPrimtValue(8)" value="8" class="btn btn-info btn-lg" style="position:relative; width:30%; height:100px; font-size:500%" />
        <input type="button" onclick="fingerPrimtValue(9)" value="9" class="btn btn-info btn-lg" style="position:relative; width:30%; height:100px; font-size:500%" />
    </p>
    <p>
        <input type="button" onclick="fingerPrimtValue(0)" value="0" class="btn btn-info btn-lg" style="position:relative; width:30%; height:100px; font-size:500%" />
        <input type="button" onclick="deleteLasFingerValue()" value="Borrar" class="btn btn-warning btn-lg" style="position:relative; width:60%; height:100px; font-size:200%" />
    </p>
</div>



