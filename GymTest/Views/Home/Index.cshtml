<script>function fingerPrimtValue(val) {
        document.getElementById("fingerprint").value += val;
        document.getElementById("fingerprint").focus();
    }

    function deleteLasFingerValue() {
        document.getElementById("fingerprint").value = document.getElementById("fingerprint").value.substring(0, document.getElementById("fingerprint").value.length - 1);
    }</script>

